{
	"fields": {
		"id": {
			"type": "integer(11)",
			"field": "id",
			"autoIncrement": true,
			"isUnique": true,
			"validators": {
				"number.number": true,
				"number.integer": true,
				"number.maximumDigits": 11
			}
		},
		"username": {
			"type": "text(50)",
			"field": "username",
			"validators": {
				"text.text": true,
				"text.minimumLength": 2,
				"text.maximumLength": 50
			}
		},
		"password": {
			"type": "text(50)",
			"field": "password",
			"validators": {
				"text.text": true,
				"text.minimumLength": 2,
				"text.maximumLength": 50
			}
		},
		"forename": {
			"type": "text(50)",
			"field": "forename",
			"validators": {
				"text.text": true,
				"text.minimumLength": 2,
				"text.maximumLength": 50
			}
		},
		"surname": {
			"type": "text(50)",
			"field": "surname",
			"validators": {
				"text.text": true,
				"text.minimumLength": 2,
				"text.maximumLength": 50
			}
		},
		"age": {
			"type": "number(3)",
			"field": "age",
			"validators": {
				"number.number": true,
				"number.integer": 0,
				"number.greaterThan": 10,
				"number.maximumDigits": 3
			}
		}
	},
	"validators": [
		{
			"rule": "comparison.equal",
			"negate": true,
			"fields": {
				"0": "username",
				"1": "password"
			},
			"options": {
				"strict": false
			}
		}
	],
	"links": {
		"friends": {
			"type": "manyToMany",
			"table": "User",
			"onInsert": "associate",
			"onUpdate": "associate",
			"onDelete": "associate",
			"via": {
				"friendLink": "UserFriend"
			},
			"joins": {
				"this.id": "userFriends.friendId1",
				"userFriends.friendId2": "friends.id"
			}
		},
		"posts": {
			"type": "oneToMany",
			"table": "Post",
			"onInsert": "insert",
			"onUpdate": "ignore",
			"onDelete": "ignore",
			"joins": {
				"this.id": "Post.authorId"
			}
		},
		"address": {
			"type": "oneToOne",
			"table": "UserAddress",
			"onInsert": "insert",
			"onUpdate": "update",
			"onDelete": "delete",
			"joins": {
				"this.id": "address.userId"
			}
		}
	}
}