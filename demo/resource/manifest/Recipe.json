{
	"name": "Recipe",
	"attributes": {
		"id": "Recipe.id",
		"name": "Recipe.name",
		"description": "Recipe.description",
		"ingredients": {
			"ingredient.id": "Ingredients.id",
			"ingredient.name": "Ingredients.name",
			"ingredient.description": "Ingredients.description",
			"ingredient.quantity": "RecipeIngredients.quantity",
			"ingredient.notes": "RecipeIngredients.notes"
		},
		"steps": {
			"step.id": "Steps.id",
			"step.number": "Steps.number",
			"step.instruction": "Steps.instruction",
			"step.notes": "Steps.notes"
		},
		"authors": {
			"author.id": "Authors.id",
			"author.name": "Authors.name"
		}
	},
	"autoAttribute": "id",
	"primaryAttribute": "name",
	"views": {
		"item": "recipe/item",
		"list": "recipe/list",
		"create": "recipe/createForm",
		"update": "recipe/updateForm",
		"simpleSearch": "recipe/simpleSearchForm",
		"search": "recipe/searchForm",
		"deleted": "recipe/deleted",
		"definition": "recipe/definition"
	},
	"tableInsertOrder": ["Recipe", "Steps", "Ingredients", "RecipeIngredients", "Authors", "RecipeAuthors"],
	"tableSelectOrder": ["Recipe", "Steps", ["RecipeIngredients", "Ingredients"], ["RecipeAuthors", "Authors"]],
	"tableDeleteOrder": ["Recipe"],
	"tables": {
		"Recipe": {
			"name": "Recipe",
			"primary": true,
			"type": "simple",
			"autoIncrement": "id",
			"primaryKey": "id",
			"attributes": {
				"id": "id",
				"name": "name",
				"description": "description"
			}
		},
		"Steps": {
			"editable": true,
			"name": "RecipeStep",
			"type": "list",
			"primaryKey": ["recipeId", "number"],
			"attributes": {
				"id": "id",
				"number": "number",
				"instruction": "instruction",
				"notes": "notes"
			},
			"links": {
				"Recipe": {
					"Recipe.id": "Steps.recipeId"
				}
			}
		},
		"RecipeIngredients": {
			"editable": true,
			"name": "RecipeIngredient",
			"type": "list",
			"primaryKey": ["recipeId", "ingredientId"],
			"attributes": {
				"ingredient.quantity": "RecipeIngredients.quantity",
				"ingredient.notes": "RecipeIngredients.notes"
			},
			"links": {
				"Recipe": {
					"Recipe.id": "RecipeIngredients.recipeId"
				}
			}
		},
		"Ingredients": {
			"editable": false,
			"name": "Ingredient",
			"type": "reference",
			"autoIncrement": "id",
			"primaryKey": "id",
			"attributes": {
				"ingredient.id": "id",
				"ingredient.name": "name",
				"ingredient.description": "description"
			},
			"links": {
				"Ingredients": {
					"RecipeIngredients.ingredientId": "Ingredients.id"
				}
			}
		},
		"RecipeAuthors": {
			"editable": true,
			"name": "RecipeAuthor",
			"type": "list",
			"primaryKey": ["recipeId", "authorId"],
			"links": {
				"Recipe": {
					"Recipe.id": "RecipeAuthors.recipeId"
				}
			}
		},
		"Authors": {
			"editable": false,
			"name": "User",
			"type": "reference",
			"autoIncrement": "id",
			"primaryKey": "id",
			"attributes": {
				"author.id": "id",
				"author.name": "name"
			},
			"links": {
				"RecipeAuthors": {
					"RecipeAuthors.authorId": "Authors.id"
				}
			}
		}
	}
}