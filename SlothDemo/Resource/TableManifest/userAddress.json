{
	"fields": {
		"userId": {
			"type": "integer(11)",
			"field": "userId",
			"validators": []
		},
		"houseName": {
			"type": "text(50)",
			"field": "houseName",
			"validators": []
		},
		"postcode": {
			"type": "text(50)",
			"field": "postcode",
			"validators": []
		},
		"landlordId": {
			"type": "integer(11)",
			"field": "landlordId",
			"validators": []
		}
	},
	"validators": [
		{
			"rule": "text.minimumLength",
			"fields": "houseName",
			"options": {
				"compareTo": 1
			}
		},
		{
			"rule": "text.maximumLength",
			"fields": "houseName",
			"options": {
				"compareTo": 50
			}
		}
	],
	"links": {
		"landlord": {
			"type": "oneToOne",
			"table": "User",
			"onInsert": "ignore",
			"onUpdate": "ignore",
			"onDelete": "ignore",
			"joins": {
				"this.landlordId": "landlord.id"
			}
		}
	}
}