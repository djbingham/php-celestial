{
	"fields": {
		"id": {
			"type": "integer(11)",
			"field": "id",
			"autoIncrement": true,
			"validators": []
		},
		"forename": {
			"type": "text(50)",
			"field": "forename",
			"validators": []
		},
		"surname": {
			"type": "text(100)",
			"field": "surname",
			"validators": []
		}
	},
	"links": {
		"address": {
			"type": "oneToOne",
			"table": "UserAddress",
			"onInsert": "insert",
			"onUpdate": "update",
			"onDelete": "delete",
			"joins": {
				"this.id": "address.userId"
			}
		},
		"posts": {
			"type": "oneToMany",
			"table": "Post",
			"onInsert": "insert",
			"onUpdate": "ignore",
			"onDelete": "ignore",
			"joins": {
				"this.id": "Post.authorId"
			}
		},
		"friends": {
			"type": "manyToMany",
			"table": "User",
			"onInsert": "associate",
			"onUpdate": "associate",
			"onDelete": "associate",
			"via": {
				"friendLink": "UserFriend"
			},
			"joins": {
				"this.id": "userFriends.friendId1",
				"userFriends.friendId2": "friends.id"
			}
		}
	},
	"validators": {}
}