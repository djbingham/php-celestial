{
	"name": "User",
	"table": "User",
	"primaryAttribute": "id",
	"attributes": {
		"id": true,
		"forename": true,
		"surname": true,
		"address": {
			"userId": true,
			"houseName": true,
			"postcode": true
		},
		"posts": {
			"id": true,
			"authorId": true,
			"content": true
		},
		"friends": {
			"id": true,
			"forename": true,
			"surname": true,
			"friends": {
				"id": true,
				"forename": true,
				"surname": true
			}
		}
	},
	"views": {
		"": {
			"engine": "php",
			"path": "resource/default/list.php"
		},
		".html": {
			"engine": "mustache",
			"path": "resource/user/list.html"
		},
		"definition": {
			"engine": "php",
			"path": "resource/default/definition.php"
		},
		"list": {
			"engine": "php",
			"path": "resource/default/list.php"
		},
